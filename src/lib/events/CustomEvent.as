package lib.events{	import flash.events.Event;		public class CustomEvent extends Event	{		protected var _note:Object;				public function CustomEvent(type:String, note:Object = null, bubbles:Boolean = false, cancelable:Boolean = true)		{			super(type, bubbles, cancelable);			_note = note;			}				public function get note():Object 		{ 			return _note; 		}				public override function clone():Event		{			return new CustomEvent(type, bubbles, cancelable, note);		}			}}